{
  "name": "multiple-angular-webpack",
  "version": "1.0.0",
  "description": "multiple angular application in single project using webpack 4+",
  "main": "index.js",
  "scripts": {
    "dev": "rm -rf dist && ./node_modules/.bin/webpack --config webpack.config.js --mode='development'",
    "dist": "rm -rf dist && ./node_modules/.bin/webpack --config webpack.config.js --mode='production' --optimize-dedupe --optimize-occurrence-order --optimize-minimize"
